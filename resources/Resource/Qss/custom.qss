

QMainWindow {
    background-color: #FFFFFF;
}

QDialog {
    background-color: #FFFFFF;
}

QTabWidget::pane{
    border: 0px;
    background-color: #FEFEFF;
}

QTabBar::tab:first{
    border-top-left-radius: 8px;

}

QTabBar::tab:last{
    border-top-right-radius: 8px;

}

QTabBar::tab:!first{
    margin-left: 2px;
    border: 0px;
}

QTabBar::tab {
    min-width: 80px;
    min-height: 35px;
    background-color: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #2E598A, stop:1 #E12E598A);
    color: #FDFDFD;
    font: 12px;
    border: 2px;
}

QTabBar::tab:hover{
    background-color: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #2196F3, stop:1 #2E598A);
    color: #FDFDFD;
}

QTabBar::tab:selected{
    background-color: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #2196F3, stop:1 #2E598A);
    color: #FDFDFD;
    font: 12px;
    font-weight: bold;
    border-bottom-right-radius: 15px;
}


QFrame#line
{
    background-color: #2E598A;
    border-radius: 2px;
    min-height: 4px;
}

