/* =============================================== */
/* Label     QToolTip                              */
/* =============================================== */
QLabel {
    background: transparent;
    border: 1px solid transparent;
    padding: 1px;
}

/* A QToolTip is a QLabel ... */
QToolTip {
    border: none;
    color: #FFFFFF;
    font-weight: bold;
    font-size: 14px;
    background: #2E598A;
    padding: 5px;
    opacity: 225;
}



/* =============================================== */
/* MenuBar Menu                                    */
/* =============================================== */
QMenuBar {
    background-color: #393E46;
    color: #FFF;
    spacing: 1px;
    border-bottom: 1px solid #999999;
    padding: 2px;
}

QMenuBar::item {
    background: transparent;
    padding: 1px 8px;
}

QMenuBar::item:selected {
    background: #222831;
    border: none;
}


QMenu {
    background-color: #393E46;
    color: #E1E1E1;
    padding: 1px;
}


QMenu::item {
    padding: 2px 25px 2px 15px;
    border: 1px solid transparent;
    border: none;
    margin: 1px;
}

QMenu::item:selected {
    border: none;
    background-color: #1E1E1E;
    color: #FFFFFF;
}

QGroupBox{
    font-size:14pt;
    font-weight: bold;
}

/* =============================================== */
/* ToolBar                                         */
/* =============================================== */
QToolBar {
    background: #FFFFFF;
    spacing: 20px;
    /* spacing between items in the tool bar */
    padding: 5px;
    border-bottom: 1px solid #999999;
}

QToolButton {
    border: none;
    border-radius: 3px;
    background: #FAFAFA;
    padding: 5px;
}

QToolButton:hover {
    /*background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0.2 #FDFDFD, stop:0.8 #2196F3F0);*/
    background: #F1F1F1;

}

QToolButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #0099CC, stop:1 #2E598A);
}

/* =============================================== */
/* QPushButton                                     */
/* =============================================== */
QPushButton {
    /*background-color: #2E598A;*/
    background-color: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #2E598A, stop:1 #E12E598A);
    color: #FDFDFD;
    border: none;
    height: 25px;
    border-radius: 2px;
    padding: 2px 10px;
}

QPushButton:disabled {
    background-color: #999999;
    color: #FDFDFD
}

QPushButton:hover {
    background-color: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #2196F3, stop:1 #2E598A);
    color: #FDFDFD;
}

QPushButton:pressed {
    background-color: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #2E598A, stop:1 #E12E598A);
    color: #E1E1E1;
}

/* =============================================== */
/* QLineEdit  QTextEdit                            */
/* =============================================== */
QLineEdit,
QPlainTextEdit,
QTextEdit {
    background: #FFF;
    selection-background-color: #1D3557;
    border: 2px solid #E1E1E1;
    border-radius: 0px;
    border-top: none;
    border-left: none;
    border-right: none;
    padding: 2px 10px;
}

QLineEdit:hover,
QPlainTextEdit:hover,
QTextEdit:hover {
    border: 2px solid #2196F3;
    border-top: none;
    border-left: none;
    border-right: none;
}

QLineEdit:focus,
QPlainTextEdit:focus,
QTextEdit:focus {
    border: 2px solid #C053297C;
    border-top: none;
    border-left: none;
    border-right: none;
}


QTextBrowser{
    /*border: 1px dashed #2196F3;*/
    border: none;
}

/* =============================================== */
/* ComboBox                                        */
/* =============================================== */
QComboBox {
    background: #FFF;
    border: 2px solid #E1E1E1;
    border-radius: 0px;
    border-left: none;
    border-right: none;
    border-top: none;
    padding: 2px 10px;
}

QComboBox:hover {
    border: 2px solid #2196F3;
    border-left: none;
    border-right: none;
    border-top: none;
}

QComboBox:focus {
    border: 2px solid #C053297C;
    border-top: none;
    border-left: none;
    border-right: none;
}

QComboBox QAbstractItemView {
    margin-top: 3px;
    background: #FFF;
}

QComboBox::drop-down {
    border: none;
}

QComboBox::down-arrow {
    image: url(:/Resource/Picture/svg/forward.svg);
    height: 15px;
    width: 15px;
}

QComboBox::down-arrow:on {
    height: 12px;
    width: 12px;
}

/* ================================================================================ */
/* SpinBox QDoubleSpinBox DateTime QDateEdit   QTimeEdit   QDateTimeEdit            */
/* ================================================================================ */
QSpinBox,
QDoubleSpinBox,
QDateEdit,
QTimeEdit,
QDateTimeEdit {
    selection-background-color: #1D3557;
    background: #FFF;
    border: 2px solid #E1E1E1;
    border-left: none;
    border-right: none;
    border-top: none;
    padding: 2px 10px;
}

QSpinBox:focus,
QDoubleSpinBox:focus,
QDateEdit:focus,
QTimeEdit:focus,
QDateTimeEdit:hover {
    border: 2px solid #C053297C;
    border-left: none;
    border-right: none;
    border-top: none;
}

QSpinBox:hover,
QDoubleSpinBox:hover,
QDateEdit:hover,
QTimeEdit:hover,
QDateTimeEdit:hover {
    border: 2px solid #2196F3;
    border-left: none;
    border-right: none;
    border-top: none;
}

QSpinBox::up-button,
QDoubleSpinBox::up-button,
QDateEdit::up-button,
QTimeEdit::up-button,
QDateTimeEdit::up-button {
    image: url(:/Resource/Picture/svg/collapse_arrow.svg);
    height: 15px;
}

QSpinBox::down-button,
QDoubleSpinBox::down-button,
QDateEdit::down-button,
QTimeEdit::down-button,
QDateTimeEdit::down-button {
    image: url(:/Resource/Picture/svg/expand_arrow.svg);
    height: 15px;
}

QSpinBox::up-button:pressed,
QDoubleSpinBox::up-button:pressed,
QDateEdit::up-button:pressed,
QTimeEdit::up-button:pressed,
QDateTimeEdit::up-button:pressed {
    height: 12px;
}

QSpinBox::down-button:pressed,
QDoubleSpinBox::down-button:pressed,
QDateEdit::down-button:pressed,
QTimeEdit::down-button:pressed,
QDateTimeEdit::down-button:pressed {
    height: 12px;
}
